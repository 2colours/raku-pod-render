{
  "name": "Raku::Pod::Render",
  "description": "A generic Pod Renderer for single or multiple files using templates, provides HTML and MarkDown",
  "version": "3.4.2",
  "perl": "6.*",
  "auth": "github:finanalyst",
  "depends": [
    "Template::Mustache",
    "URI",
    "LibCurl",
    "Pod::Load",
    "GTK::Simple",
    "File::Temp"
  ],
  "build-depends": [],
  "test-depends": [
    "Test",
    "Test::Output",
    "Test::META",
    "Test::Deeply::Relaxed"
  ],
  "provides": {
    "Pod::To::HTML": "lib/Pod/To/HTML.pm6",
    "Pod::To::HTML::Mustache": "lib/Pod/To/HTML.pm6",
    "Pod::To::MarkDown": "lib/Pod/To/MarkDown.pm6",
    "ProcessedPod": "lib/ProcessedPod.pm6",
    "Extractor.raku": "bin/Extractor.raku",
    "ExtractPod": "lib/ExtractPod.pm6",
    "test-templates": "bin/test-templates.raku"
  },
  "resources": [
    "pod.css",
    "Camelia.svg",
    "favicon.ico",
    "camelia-ico.bin",
    "closure-temp.raku",
    "extra-test.raku",
    "highlights/highlight-filename-from-stdin.coffee",
    "highlights/package.json"
  ],
  "support": {
    "source": "https://github.com/finanalyst/raku-pod-render.git",
    "bugtracker": "https://github.com/finanalyst/raku-pod-render/issues",
    "email": "richard@hainsworth.wales"
  },
  "license": "Artistic-2.0",
  "source-url": "git://github.com/finanalyst/raku-pod-render.git",
  "authors": [ "Richard Hainsworth" ],
  "source-type": "git"
}