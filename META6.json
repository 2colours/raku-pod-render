  "version": "2.1",
  "perl": "6.*",
  "auth": "github:finanalyst",
  "depends": [
    "Template::Mustache:auth<github.finanalyst>",
    "URI",
    "LibCurl"
  ],
  "build-depends": [],
  "test-depends": [
    "Test",
    "Test::Output"
  ],
  "provides": {
    "Pod::To::HTML": "lib/Pod/To/HTML.pm6",
    "Pod::To::MarkDown": "lib/Pod/To/MarkDown.pm6",
    "ProcessedPod": "lib/ProcessedPod.pm6"
  },
  "resources": [
    "pod.css"
  ],
  "support": {
    "source": "https://github.com/finanalyst/raku-pod-render"
  },
  "license": "Artistic-2.0",
  "source-url": "git://github.com/finanalyst/raku-pod-render.git",
  "author": "Richard Hainsworth",
  "source-type": "git"
}