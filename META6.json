{
  "name": "Raku::Pod::Render",
  "description": "A generic Pod6 Renderer with custom Pod::Blocks, FormatCodes for one or more files using templates, provides HTML and MarkDown.",
  "version": "3.7.5",
  "perl": "6.d",
  "authors": [
    "Richard Hainsworth"
  ],
  "auth": "github:finanalyst",
  "depends": [
    "Template::Mustache",
    "Cro::WebApp::Template::Repository::Hash",
    "Cro::WebApp",
    "URI",
    "LibCurl",
    "Pod::Load",
    "Test::Deeply::Relaxed",
    "File::Directory::Tree",
    "Test::Output",
    "File::Temp",
    "Test::META",
    "PrettyDump",
    "JSON::Fast"
  ],
  "build-depends": [],
  "test-depends": [
    "Test"
  ],
  "provides": {
    "ExtractPod": "lib/ExtractPod.rakumod",
    "InstallAtomHighlighter": "lib/InstallAtomHighlighter.rakumod",
    "Pod::To::HTML2": "lib/Pod/To/HTML2.rakumod",
    "Pod::To::HTML2::Mustache": "lib/Pod/To/HTML2.rakumod",
    "Pod::To::MarkDown": "lib/Pod/To/MarkDown.rakumod",
    "ProcessedPod": "lib/ProcessedPod.rakumod",
    "RenderPod::Exceptions": "lib/RenderPod/Exceptions.rakumod",
    "RenderPod::Highlighting": "lib/RenderPod/Highlighting.rakumod",
    "RenderPod::Templating": "lib/RenderPod/Templating.rakumod",
    "RenderPod::Test-Templates": "lib/RenderPod/Test-Templates.rakumod",
    "raku-pod-render-install-highlighter": "bin/raku-pod-render-install-highlighter",
    "test-templates": "bin/test-templates"
  },
  "resources": [
    "pod.css",
    "Camelia.svg",
    "favicon.ico",
    "camelia-ico.bin",
    "html-rakuclosure.raku",
    "extra-test.raku",
    "highlights/highlight-filename-from-stdin.coffee",
    "highlights/package.json"
  ],
  "support": {
    "bugtracker": "https://github.com/finanalyst/raku-pod-render/issues",
    "email": "richard@hainsworth.wales",
    "source": "https://github.com/finanalyst/raku-pod-render.git"
  },
  "license": "Artistic-2.0",
  "source-url": "git://github.com/finanalyst/raku-pod-render.git",
  "source-type": "git"
}