{
  "name": "Raku::Pod::Render",
  "description": "A generic Pod Renderer for single or multiple files using templates, provides HTML and MarkDown",
  "version": "2.3.1",
  "perl": "6.*",
  "auth": "github:finanalyst",
  "depends": [
    "Template::Mustache",
    "URI",
    "LibCurl",
    "Pod::Load"
  ],
  "build-depends": [],
  "test-depends": [
    "Test",
    "Test::Output",
    "Test::META",
    "Test::Deeply::Relaxed"
  ],
  "provides": {
    "Pod::To::HTML": "lib/Pod/To/HTML.pm6",
    "Pod::To::MarkDown": "lib/Pod/To/MarkDown.pm6",
    "Pod::To::HTML2": "lib/Pod/To/HTML2.pm6",
    "Pod::To::MarkDown2": "lib/Pod/To/MarkDown2.pm6",
    "ProcessedPod": "lib/ProcessedPod.pm6",
    "read-y-me.raku": "bin/read-y-me.raku"
  },
  "resources": [
    "pod.css",
    "Camelia.svg",
    "favicon.ico",
    "camelia-ico.bin"
  ],
  "support": {
    "source": "https://github.com/finanalyst/raku-pod-render.git",
    "bugtracker": "https://github.com/finanalyst/raku-pod-render/issues",
    "email": "richard@hainsworth.wales"
  },
  "license": "Artistic-2.0",
  "source-url": "git://github.com/finanalyst/raku-pod-render.git",
  "authors": [ "Richard Hainsworth" ],
  "source-type": "git"
}